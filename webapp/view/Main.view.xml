<mvc:View controllerName="oup.ptp.zptpplannerscockpit.controller.Main"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns:l="sap.ui.layout" displayBlock="true"
    xmlns="sap.m"
    xmlns:smartFilterBar="sap.ui.comp.smartfilterbar"
    xmlns:smartTable="sap.ui.comp.smarttable"
    xmlns:navpopover="sap.ui.comp.navpopover"
    xmlns:table="sap.ui.table"
    xmlns:f="sap.f">
    <f:DynamicPage  >
        <!-- DynamicPage Title -->
        <f:title>
            <f:DynamicPageTitle>
                <f:heading>
                    <VBox id="dynamicRadioBtn" fitContainer="true">
                    </VBox>
                </f:heading>
            </f:DynamicPageTitle>
        </f:title>
        <!-- DynamicPage Header -->
        <f:header>
            <f:DynamicPageHeader pinnable="true">
                <VBox id="dynamicTableIdFilter" fitContainer="true" />
            </f:DynamicPageHeader>
        </f:header>
        <f:content>
            <VBox id="dynamicTableId" fitContainer="true" />
        </f:content>
        <!-- DynamicPage Footer -->
    </f:DynamicPage>
</mvc:View>
