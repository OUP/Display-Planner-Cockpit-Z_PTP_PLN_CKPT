<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
    <Dialog afterClose="afterClose" beforeOpen="beforeOpen" class="sapUiPopupWithPadding" title="Margin Cost">
        <VBox class="sapUiSmallMargin">
            <f:Form id="FormChange354" editable="true">
                <f:layout>
                    <f:ResponsiveGridLayout labelSpanXL="3" labelSpanL="3" labelSpanM="3" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="4" emptySpanL="4" emptySpanM="4" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1" singleContainerFullSize="false" />
                </f:layout>
                <f:formContainers>
                    <f:FormContainer>
                        <f:formElements>
                            <f:FormElement label="First Cost Total">
                                <f:fields>
                                    <Input id="idFirstCost" value="" />
                                </f:fields>
                            </f:FormElement>
                            <f:FormElement label="PPB Cost Total">
                                <f:fields>
                                    <Input id="idPPBCost" value="" />
                                </f:fields>
                            </f:FormElement>
                            <f:FormElement label="Freight Total">
                                <f:fields>
                                    <Input id="idFreight" value="" />
                                </f:fields>
                            </f:FormElement>
                            <f:FormElement label="Royalty">
                                <f:fields>
                                    <Input id="idRoyalty" value="" />
                                </f:fields>
                            </f:FormElement>
                            <f:FormElement label="Discount">
                                <f:fields>
                                    <Input id="idDiscount" value="" />
                                </f:fields>
                            </f:FormElement>
                            <f:FormElement label="Unit Sale">
                                <f:fields>
                                    <Input id="idUnitSale" value="" />
                                </f:fields>
                            </f:FormElement>
                            <f:FormElement label="Retail Price">
                                <f:fields>
                                    <Input id="idRetailPrice" value="" />
                                </f:fields>
                            </f:FormElement>
                        </f:formElements>
                    </f:FormContainer>
                </f:formContainers>
            </f:Form>
        </VBox>

        <beginButton>
            <Button press="onOKMarginPressed" text="OK" />
        </beginButton>
        <endButton>
            <Button press="onCancelMarginPressed" text="Cancel" />
        </endButton>
    </Dialog>
</core:FragmentDefinition>
